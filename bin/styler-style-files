#!/usr/bin/env Rscript
print("libPaths are") 
print(.libPaths())
print("wd is")
print(getwd())
print("user rprofile exists:")
print(file.exists(".Rprofile"))
print("global R profile exists:")
print(file.exists("~/.Rprofile"))
print("HOme is at:")
print(system2("echo", "$HOME"))
stop("now you saw home and rpfoeiles ect.")